@using Core.Web.Helpers
@model MyWeb.ViewModels.euser.moduleModel
@{
    var hapusModel = Html.Angular().ModelFor("vm.module");
}
<form novalidate
      name="vm.form"
      ng-submit="vm.save()">
    <fieldset ng-disabled="vm.saving">
        <div class="modal-header">
            <h3 class="modal-title">{{vm.title}} Module Ini ?</h3>
        </div>

        <div class="modal-body">
            <table class="table">
                <tr>
                    <th>Username</th>
                    <td>
                        @hapusModel.BindingFor(x => x.Username)
                    </td>
                </tr>
                <tr>
                    <th>Module</th>
                    <td>
                        @hapusModel.BindingFor(x => x.Module)
                    </td>
                </tr>
            </table>
        </div>

        <div class="modal-footer">
            <div class="row">
                <div class="col-lg-12">
                    @Html.Angular().AlertGlobal("vm.message")
                </div>
            </div>
            <button class="btn btn-success">Delete</button>
            <button type="button" class="btn btn-warning" ng-click="$parent.$dismiss()">Cancel</button>
        </div>

    </fieldset>
</form>